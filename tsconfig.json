{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
  },
  "include": [
      "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
